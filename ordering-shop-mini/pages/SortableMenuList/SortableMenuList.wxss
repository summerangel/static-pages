.menu-list-wrap {
   padding-left: 40rpx;
   padding-right: 40rpx;
   border-bottom: 2rpx solid #F6F6F6;
}
.menu-list {
  height: 62px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.menu-list .menu-icon {
  display: flex;
  align-items: center;
}
.menu-list .fold {
  color: #5B9DEA;
  font-size: 16rpx;
}
.menu-list .fold.expand {
  transform: rotate(90deg);
}
.menu-list .item-name {
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
  margin-left: 18rpx;
  display: inline-block;
}
.menu-list .item-num {
  font-size: 32rpx;
  color: #999999;
}

.goods-info {
  padding-top: 30rpx;
  display: flex;
  justify-content: flex-start;
  z-index: 999;
}
.goods-info .icon {
  flex: 0 0 114rpx;
}
.goods-info .goods-right {
  height: 180rpx;
  padding-left: 30rpx;
  width: 100%;

  position: relative;
}
.goods-info .goods-right .goods-name {
  font-size: 32rpx;
  color: #333333;
}
.goods-info .goods-right .price {
  font-size: 36rpx;
  color: #DC4950;

  position: absolute;
  left: 30rpx;
  bottom: 0;
}
/* 新增修改,包括字体  */
.goods-info .goods-right .iconfont {
  font-size: 34rpx;
}
.goods-info .goods-right .active {
  color: #5B9DEA;
  font-size: 40rpx;
  margin-top: 20rpx;
}
.goods-info .goods-right .disable {
  color: #D6D6D6;
  font-size: 40rpx;
  margin-top: 20rpx;
}
.goods-info .goods-right .unshelf-btn {
  /* width: 166rpx;
  height: 60rpx; */
  /* line-height: 60rpx; */
  /* text-align: center; */
  /* border: 2rpx solid #BDBDBD; */
  /* border-radius: 200rpx; */
  /* font-size: 26rpx; */
  color: #666666;

  position: absolute;
  right: 80rpx;
  bottom: 0;
}
.goods-info .goods-right .edit-btn {
  color: #666666;

  position: absolute;
  right: 0;
  bottom: 0;
}


