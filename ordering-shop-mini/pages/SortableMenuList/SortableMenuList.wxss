/* 头部 */
.menu-tab-wrap {
  height:67px;
  border-bottom: 2rpx solid #F6F6F6;
  position: relative;
}
.menu-tab-wrap .menu-tab {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-top: 14rpx;
  padding-left: 40rpx;
}
.menu-tab-wrap .menu-goods {
  font-size: 40rpx;
  color: #BDBDBD;
  padding-bottom: 20rpx;
  padding-right: 50rpx;
  font-weight: bold;
  position: relative;
}
.menu-tab-wrap .menu-goods.active {
  color: #333333;
}
.menu-tab-wrap .menu-goods.active .tab-underline {
  width: 68rpx;
  height: 8rpx;
  background-color: #333333;
  border-radius: 8rpx;
  
  position: absolute;
  bottom: 0;
  left: 22%;
}
.menu-tab-wrap .menu-add-btn {
  position: absolute;
  top: 5%;
  right: 40rpx;
   color: #FDCB47; 
  font-size: 80rpx;
}

.touchView {
    z-index: 200;
    /* width: 100px; */
    width: 60%;
    position: absolute;
    top: 0;
    /* left: 275px; */
    right: 0;
}

